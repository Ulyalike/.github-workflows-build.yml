имя: Java CI с Maven

вкл:
  толкать:
    ветки: [ "главная" ]
  pull_request:
    ветки: [ "главная" ]

Вакансии:
  сборка:

    запущено: ubuntu-последняя версия

    шаги:
 - использование: действия / оформить заказ@v3
 - имя: Настройка JDK 17
        использование: действия / настройка-java@v3
        с:
          java-версия: '17'
          рассылка: 'темурин'
          кэш: maven
 - имя: Сборка с помощью Maven
        выполнить: пакет mvn
